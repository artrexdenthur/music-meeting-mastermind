<%= render 'layouts/model_errors', model: @singer %>
<%= form_for @singer, url: url_for(controller: 'members', action: 'update') do |f| %>
  <%= render 'singers/form', f: f %>
<% end %>
<%= form_for @singer, method: :delete do |f| %>
  <%= f.button "Delete Singer", class: "btn red" %>
<% end %>

